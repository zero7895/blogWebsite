<%= simple_form_for(@game_datum) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :game_info_id, collection: GameInfo.all, label_method: :info %>
    <%= f.input :money1 %>
    <%= f.input :money2 %>
    <%= f.input :money3 %>
    <%= f.input :money4 %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
