<%= simple_form_for(game_datum) do |f| %>
  <%= f.error_notification %>

  <div class="row">
    <%= f.hidden_field :game_info_id, :value => game_info_id %>
    <div class="col-xs-2">
      <%= f.input :money1 %>
    </div>
    <div class="col-xs-2">
        <%= f.input :money2 %>
    </div>
    <div class="col-xs-2">
      <%= f.input :money3 %>
    </div>
    <div class="col-xs-2">
      <%= f.input :money4 %>
    </div>
    <div class="col-xs-2 "> 
      <%= f.button :submit, class: 'btn btn-success' %>
    </div>
  </div>


<% end %>
